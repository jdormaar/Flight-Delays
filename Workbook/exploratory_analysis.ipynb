    "def WorldWeatherOnlineAPI(city, date, time):\n",
    "  '''\n",
    "  input:\n",
    "  output:  \n",
    "  '''\n",
    "  api_key = os.environ['WEATHER_API_KEY']\n",
    "  params = {\n",
    "    'q': city\n",
    "    , 'date': date\n",
    "    , 'format': 'json'\n",
    "    , 'key': api_key\n",
    "  }\n",
    "  # q={dest_cities}&date={date}&format=json&key={api_key}'\n",
    "\n",
    "  wwo_url = f'https://api.worldweatheronline.com/premium/v1/past-weather.ashx?'\n",
    "  wwoHxWeather_json = requests.get(wwo_url, params=params).json()\n",
    "\n",
    "  list_of_dict = []\n",
    "  for i in wwoHxWeather_json['data']:\n",
    "\n",
    "    weather_dict = {\n",
    "          'maxtempC':      i['weather']\n",
    "        , 'mintempC':      i['weather']\n",
    "        , 'avgtempC':      i['weather']\n",
    "        , 'totalSnow_cm':  i['weather']\n",
    "        , 'sunHour':       i['weather']\n",
    "        , 'uvIndex':       i['weather']\n",
    "        , 'hourly':        i['weather']\n",
    "        , 'WindChillC':     i['weather']['hourly'][0]\n",
    "        , 'WindGustKmph':     i['weather']['hourly'][0]\n",
    "        , 'cloudcover':     i['weather']['hourly'][0]\n",
    "        , 'precipMM':     i['weather']['hourly'][0]\n",
    "        , 'pressure':     i['weather']['hourly'][0]\n",
    "        , 'tempC':      i['weather']['hourly'][0]\n",
    "        , 'time':     i['weather']['hourly'][0]\n",
    "        , 'uvIndex':      i['weather']['hourly'][0]\n",
    "        , 'visibility':     i['weather']['hourly'][0]\n",
    "        , 'weatherCode':      i['weather']['hourly'][0]\n",
    "        , 'weatherDesc':      i['weather']['hourly'][0]\n",
    "        , 'winddir16Point':     i['weather']['hourly'][0]\n",
    "        , 'winddirDegree':      i['weather']['hourly'][0]\n",
    "        , 'windspeedKmph':      i['weather']['hourly'][0]\n",
    "        , 'weather_type':      i['weather']['hourly'][0]['weatherDesc'][time[i]]\n",
    "    }\n",
    "  list_of_dict.append(weather_dict)\n",
    "  return pd.DataFrame(list_of_dict)\n",
    "\n",
    "    "
   ]
